modules = ["python-3.11", "python3", "postgresql-16"]

[nix]
channel = "stable-24_05"
packages = ["cairo", "ffmpeg-full", "freetype", "geos", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "pkg-config", "proj", "qhull", "tcl", "tk", "xsimd", "zlib"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Start Groundwater App"
author = 42797086
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install --upgrade pip"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install streamlit folium streamlit-folium scipy pyproj pykrige scikit-learn matplotlib pandas numpy requests geopandas --no-cache-dir"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0 --server.headless true --server.enableCORS false --server.enableXsrfProtection false"

[[workflows.workflow]]
name = "Mobile-Friendly Groundwater App"
author = 42797086
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install --upgrade pip"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install streamlit folium streamlit-folium scipy pyproj pykrige scikit-learn matplotlib pandas numpy requests geopandas psycopg2-binary sqlalchemy --no-cache-dir"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0 --server.headless true --server.enableCORS false --server.enableXsrfProtection false --server.allowRunOnSave true"

[[workflows.workflow]]
name = "Fixed Groundwater App"
author = 42797086
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0 --server.headless true --server.enableCORS false --server.enableXsrfProtection false"

[[workflows.workflow]]
name = "Restart Groundwater App"
author = 42797086
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f streamlit"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "sleep 2"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0 --server.headless true --server.enableCORS false --server.enableXsrfProtection false"

[[workflows.workflow]]
name = "Stable Restart"
author = 42797086
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f streamlit"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "sleep 3"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0 --server.headless true --server.enableCORS false --server.enableXsrfProtection false"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Stable Groundwater App"

[[workflows.workflow]]
name = "Stable Groundwater App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0 --server.headless true"
waitForPort = 5000

